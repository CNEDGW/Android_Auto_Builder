[
  {
    "kernelSource": {
      "name": "fogos_krl",
      "repo": "https://github.com/Motorola-SM6375-Devs/android_kernel_motorola_sm6375",
      "branch": "fourteen",
      "device": "fogos",
      "defconfig": "."
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master-kernel-build-2022/clang-r450784d.tar.gz",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "gcc",
        "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_COMPAT": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "",
        "branch": ""
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
