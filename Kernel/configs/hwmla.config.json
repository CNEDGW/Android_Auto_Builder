[
  {
    "kernelSource": {
      "name": "hwmla",
      "repo": "https://github.com/CNEDGW/android_kernel_hwcan",
      "branch": "lineage-15.1",
      "device": "hwmla",
      "defconfig": "."
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_COMPAT": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "",
        "branch": ""
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
